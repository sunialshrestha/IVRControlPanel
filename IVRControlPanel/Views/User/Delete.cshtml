@model IVRControlPanel.Models.User
@{
    ViewBag.Title = "Delete User";
}
<h3>
    Are you sure you want to delete this?</h3>
<div class="display-label">
    Role: @Html.DisplayFor(model => model.Role.Name)</div>
<div class="display-label">
    Name: @Html.DisplayFor(model => model.Name)</div>
<div class="display-label">
    UserName: @Html.DisplayFor(model => model.UserName)
</div>
<div class="display-label">
    Email: @Html.DisplayFor(model => model.Email)</div>
@using (Html.BeginForm())
{
    <input type="submit" value="Delete" class="bt red" />
    <header class="grid_12 head">
        @Html.ActionLink("Back to List", "Index")
    </header>
}
