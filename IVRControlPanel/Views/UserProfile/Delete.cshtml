@model IVRControlPanel.Models.User

@{
    ViewBag.Title = "Delete User";
}

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <div class="display-label">Role:   @Html.DisplayFor(model => model.Role.Name)</div>
    <div class="display-label">Name:   @Html.DisplayFor(model => model.Name)</div>
     <div class="display-label">UserName: @Html.DisplayFor(model => model.UserName)</div>
     <div class="display-label">Email:    @Html.DisplayFor(model => model.Email)</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
