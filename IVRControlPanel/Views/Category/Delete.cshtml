@model IEnumerable<IVRControlPanel.Models.Category>

@{
    ViewBag.Title = "Delete Category";
}


<h3>Are you sure you want to delete this?</h3>
<fieldset>
 @foreach (var item in Model)
 {
    <div class="display-label">CategoryName: @item.CategoryName</div>
 }
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" class="bt red"/>
         <header class="grid_12 head">
        @Html.ActionLink("Back to List", "Index")
        </header>
    </p>
}
